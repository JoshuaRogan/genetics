{
	"name": "geneticsv2",
	"version": "1.0.0",
	"description": "Static JS version of genetics apo",
	"main": "index.js",
	"license": "MIT",
	"dependencies": {
		"debounce": "^1.2.1",
		"highcharts": "^9.3.2",
		"highcharts-react-official": "^3.1.0",
		"lint-staged": "^12.1.4",
		"next": "^12.0.7",
		"prettier": "^2.5.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-icons": "^4.3.1",
		"react-slider": "^1.3.1",
		"styled-components": "^5.3.3"
	},
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "prettier components pages *.js* -w && next lint"
	},
	"devDependencies": {
		"@types/react": "^17.0.37",
		"eslint": "8.4.1",
		"eslint-config-next": "12.0.7",
		"eslint-config-prettier": "^8.3.0",
		"typescript": "^4.5.4"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"components/**/*.(ts|tsx|js)": [
			"prettier --write",
			"eslint --fix"
		],
		"pages/**/*.(ts|tsx|js)": [
			"prettier --write",
			"eslint --fix"
		]
	}
}
